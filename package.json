{
  "name": "dark-party",
  "displayName": "Dark Party",
  "description": "VSCode dark theme based on dracula theme. For personal use, feel free to download if you want to use it too.",
  "version": "0.0.1",
  "publisher": "ryuukibeat",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryuukibeat/Dark-Party.git"
  },
  "bugs": {
    "url": "https://github.com/ryuukibeat/Dark-Party/issues"
  },
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [ "Themes" ],
  "contributes": {
    "themes": [
      {
        "label": "Dark Party",
        "uiTheme": "vs-dark",
        "path": "./themes/Dark-Party.color-theme.json"
      },
      {
        "label": "Dark Party Soft",
        "uiTheme": "vs-dark",
        "path": "./themes/Dark-Party-Soft.color-theme.json"
      }
    ]
  },
  "scripts": {
    "attach": "node ./scripts/bootstrap.js attach",
    "build": "node ./scripts/build.js",
    "lint": "node ./scripts/lint.js",
    "eject": "node ./scripts/bootstrap.js eject",
    "package": "vsce package -o ./bin/dark-party.vsix",
    "vscode:prepublish": "npm run build"
  },
  "icon": "icon.png",
  "keywords": [ "dark party", "dracula", "custom theme", "theme", "vscode" ],
  "devDependencies": {
    "js-yaml": "^3.12.0",
    "tinycolor2": "^1.4.1"
  }
}
